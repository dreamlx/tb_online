<h1><%=_ "Editing %s", Expense.human_name %></h1>

<%= error_messages_for :expense %>

<% form_for(:expense, :url => expense_path(@expense), :html => { :method => :put }) do |f| %>
<%= render :partial => 'form',:locals => { :f => f }%>
  <p>
    <%= submit_tag _("Update") %>
  </p>
<% end %>

<%= link_to _('View'), expense_path(@expense) %> |
<%= link_to 'Back', :action => 'index', :prj_id => @expense.project_id %>
