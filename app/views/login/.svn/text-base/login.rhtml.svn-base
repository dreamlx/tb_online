<h1>Login#login</h1>

<%= error_messages_for 'user' %>

<%= form_tag %>

<table>

<tr>

<td>User name:</td>

<td><%= text_field("user", "name") %></td>

</tr>

<tr>

<td>Password:</td>

<td><%= password_field("user", "password") %></td>

</tr>

<tr>

<td></td>

<td><input type="submit" value=" Login " /></td>

</tr>

</table>

<%= end_form_tag %>

